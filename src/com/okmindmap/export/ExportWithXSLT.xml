<?xml version="1.0"?>
<export label="com/okmindmap/export/ExportWithXSLT">
	<export_action name="As XHTML (JavaScript version)..." label="com/okmindmap/export/ExportWithXSLT_HTML.properties">
		<export_property name="file_type" value="html"/>
		<export_property name="xslt_file" value="toxhtml.xsl"/>
		<export_property name="create_dir" value="true"/>
		<export_property name="files_to_copy" value="marktree.js,minus.png,plus.png,ilink.png,treestyles.css"/>
		<export_property name="file_prefix" value="accessories/"/>
		<export_property name="copy_icons" value="true"/>
		<export_property name="load_file" value="true"/>
		<export_property name="create_html_linked_image" value="false"/>
	</export_action>
    <export_action name="TWiki File (*.twi)" label="com/okmindmap/export/ExportWithTWiki.properties">
		<export_property name="file_type" value="twi"/>
        <export_property name="name_extension" value=""/>
		<export_property name="xslt_file" value="mm2twiki.xsl"/>
		<export_property name="copy_map" value="false"/>
		<export_property name="load_file" value="false"/>
		<export_property name="create_dir" value="false"/>
		<export_property name="create_html_linked_image" value="false"/>
	</export_action>
</export>


